    NSMutableArray      *_cityList;
    NSMutableDictionary *_areaList;
    
    
    int _isXmlOk;
    CGPoint     _tableCenter;
    NSString *_orderNo;
    NSString *_password;
    NSString *_username;
    
    BR_Area *_currentCity;
    BR_Area *_currentArea;
    
    
